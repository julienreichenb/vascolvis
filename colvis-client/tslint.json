{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "curly": [
            true,
            "as-needed"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "member-access": false,
        "interface-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false
    }
}